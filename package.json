{
  "name": "hunters-guide",
  "version": "1.1.0",
  "private": true,
  "description": "Mobile Hunter's Guide for Monster Hunter World",
  "repository": "",
  "license": "MIT",
  "author": "C. Yarbrough <cryarbrough@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "coverage": "cross-env COVERAGE=true ember test",
    "deploy-prod": "git push heroku master",
    "install-app": "npm i",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "update-app": "rm -rf node_modules dist tmp && npm i"
  },
  "dependencies": {
    "js-base64": "^2.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^1.3.0",
    "@ember/string": "^3.0.1",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "broccoli-asset-rev": "^2.4.5",
    "concurrently": "^8.0.1",
    "cross-env": "^5.1.3",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "^4.12.1",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-content-security-policy": "^1.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-stylelint": "^4.0.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-cookies": "^1.1.1",
    "ember-data": "~4.12.1",
    "ember-fetch": "^8.1.2",
    "ember-in-viewport": "^4.1.0",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-radio-button": "^3.0.0-beta.1",
    "ember-resolver": "^10.0.0",
    "ember-service-worker": "^9.0.1",
    "ember-source": "~4.12.0",
    "ember-template-lint": "^5.13.0",
    "ember-web-app": "^2.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.2.3",
    "normalize.css": "^8.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.4.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.3.1",
    "tracked-built-ins": "^3.1.1",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
