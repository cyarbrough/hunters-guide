{{header-nav 
  sidePanelIsOpen=sidePanelIsOpen
  searchTerm=(readonly searchTerm) 
  actionSearch=(action "updateSearchTerm")
  actionButton=(action "toggleSidePanel")
}}
<div class="container">
  {{#side-panel isOpen=sidePanelIsOpen actionClose=(action "toggleSidePanel")}}
    {{outlet "side-panel"}}
  {{/side-panel}}

  {{outlet}}
  <div class="monster-list">
    {{#each monsterList as |monster|}}
      {{monster-row monster=monster logEvent=(action 'logEvent')}}
    {{/each}}
  </div>
</div>
{{footer-nav}}